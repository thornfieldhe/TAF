@using TAF.MVC
@using TAF.Utility
@using TAF.Web.Models
<div class="row" id="main">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">字典管理</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <add-button title="新增"></add-button>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th>
                                        键
                                    </th>
                                    <th>
                                        值
                                    </th>
                                    <th>
                                        值1
                                    </th>
                                    <th>
                                        值2
                                    </th>
                                    <th>
                                        值3
                                    </th>
                                    <th>
                                        值4
                                    </th>
                                    <th>
                                        值5
                                    </th>
                                    <th>
                                        值6
                                    </th>
                                    <th>
                                        值7
                                    </th>
                                    <th>
                                        值8
                                    </th>
                                    <th>
                                        值9
                                    </th>
                                    <th style="width: 180px;">
                                        操作
                                    </th>
                                </tr>
                                <tr role="row">
                                    <th>
                                        <input type="text" class="form-control" placeholder="键" v-model="queryEntity.key" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值" v-model="queryEntity.value"/>
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值1" v-model="queryEntity.value1" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值2" v-model="queryEntity.value2" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值3" v-model="queryEntity.value3" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值4" v-model="queryEntity.value4" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值5" v-model="queryEntity.value5" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值6" v-model="queryEntity.value6" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值7" v-model="queryEntity.value7" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值8" v-model="queryEntity.value8" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="值9" v-model="queryEntity.value9" />
                                    </th>
                                    <th><search-command></search-command></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for=" it in list.Datas">
                                    <td>{{it.Key}}</td>
                                    <td>{{it.Value}}</td>
                                    <td>{{it.Value1}}</td>
                                    <td>{{it.Value2}}</td>
                                    <td>{{it.Value3}}</td>
                                    <td>{{it.Value4}}</td>
                                    <td>{{it.Value5}}</td>
                                    <td>{{it.Value6}}</td>
                                    <td>{{it.Value7}}</td>
                                    <td>{{it.Value8}}</td>
                                    <td>{{it.Value9}}</td>
                                    <td>
                                        <row-command :id="it.Id" title="编辑" :name="it.Value"></row-command>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot is="table-foot" colspan="12"></tfoot>
                        </table>
                    </div>
                </div>
                <form-edit></form-edit>
                <dialog-delete delete-url="/Dictionary/Delete/"></dialog-delete>
            </div>
        </div>
    </div>
</div>
<template id="dictionaryFormBody">

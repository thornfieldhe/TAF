                    <div class="col-sm-10">
                        <div class="row">
                            <select name="key" style="width: 100%" v-model="item.Key">
                                <option value="">请选择...</option>
                                @{
                                    foreach (var  item in EnumExt.GetItems<DictionaryKey>())
                                    {
                                        <option value="@item.Value">@item.Text</option>
